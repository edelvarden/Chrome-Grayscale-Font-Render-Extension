{
  "name": "font-customizer",
  "displayName": "Font-Customizer",
  "version": "2.8.3",
  "author": "https://github.com/edelvarden",
  "description": "",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "fmt": "prettier --write 'src/**/*.{ts,js,json,css,scss,md}'",
    "zip": "pnpm run build && node scripts/zip.js"
  },
  "lint-staged": {
    "*.ts": [
      "tsc --noEmit"
    ],
    "*.{js,ts,css,md}": [
      "prettier --write --list-different"
    ]
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^1.0.14",
    "@types/chrome": "^0.0.268",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "path": "^0.12.7",
    "prettier": "^3.3.2",
    "typescript": "^5.5.2",
    "vite": "^4.5.3",
    "vite-tsconfig-paths": "^4.3.2",
    "zip-a-folder": "^3.1.7"
  },
  "dependencies": {
    "@material/web": "^1.5.0",
    "lit": "^3.1.4"
  }
}
