{
  "name": "font-customizer",
  "displayName": "Font Customizer",
  "version": "2.9.4",
  "author": "https://github.com/edelvarden",
  "description": "",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "pnpm test && tsc && vite build",
    "preview": "vite preview",
    "fmt": "prettier --write \"src/**/*.{ts,js,json,css,md}\"",
    "zip": "pnpm run build && node scripts/zip.js",
    "test": "jest --testPathPattern=\"src/__tests__\"",
    "bench": "jest --runInBand \"src/__benchmarks__/benchmark.test.ts\""
  },
  "lint-staged": {
    "*.{js,ts,css,md}": [
      "prettier --write --list-different"
    ]
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^1.0.14",
    "@types/chrome": "^0.0.268",
    "@types/jest": "^29.5.12",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.7",
    "path": "^0.12.7",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2",
    "vite": "^4.5.3",
    "vite-tsconfig-paths": "^4.3.2",
    "zip-a-folder": "^3.1.7"
  },
  "dependencies": {
    "@material/web": "^1.5.0",
    "lit": "^3.1.4"
  }
}
